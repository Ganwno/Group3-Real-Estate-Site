<% if @prev_loc.length != 0  %>
	<div id="header-nav-links">
		<%# Back Link %>
		<%= link_to url_for("javascript:history.back()"), id:"back-search-link" do %>
			<% case @prev_loc
				when /search/ %>
					< back to search results
				<% when /buy/ %>
					< back to homepage
				<% when /sell/ %>
					< back to manage properties
				<% when /dashboard/ %>
					< back to your dashboard
				<% else %>
					< back to previous property
			<% end %>
		<% end %>
		<%# Prev/Next Tags %>
		<span id="next-prev-links">
			<%= link_to "< previous", buy_path %>
			<%= link_to "next >", buy_path %>
		</span>
	</div>
<% end %>