<%# Address and Bed Bath and Beyond Property Details %>
<h1 class="ui header">
	88 Millne Rd Bays Water WA 6053
	<div class="property-bbb">
		<%# num of bedrooms %>
		<i class="large bed icon"></i> <span>2</span>
		<%# num of bathrooms %>
		<i class="shower"><%= image_tag "icon_shower_dark_small.png", alt: "shower icon" %></i> <span>1</span>
		<%# num of carspaces %>
		<i class="large car icon"></i> <span>3</span>
	</div>
</h1>
<%# Price and some status %>
<h2 class="ui header">Under offer for $1,234,567</h2>
<%# Tags Area %>
<div class="extra content tag-field">
	<div class="ui large transparent left">
		<%# tags:
			sell-feature-tag is purple
			location-feature-tag & property-feature-tag is blue
		 	price-feature-tag is red %>
		<div class="ui property-feature-tag label">properrty feature</div>
		<div class="ui property-feature-tag label"> sqm</div>
		<div class="ui sell-feature-tag label">Home Theatre</div>
		<div class="ui sell-feature-tag label">NBN</div>
		<div class="ui sell-feature-tag label">Pool</div>
		<div class="ui sell-feature-tag label">Front Garden</div>
		<div class="ui sell-feature-tag label">Back Garden</div>
		<div class="ui location-feature-tag label">1.3km from your Work</div>
		<div class="ui property-feature-tag label">5.1km from Schools</div>
		<div class="ui location-feature-tag label">2.3km from Shopping Centre</div>
		<% # Get the tag list in a readable format (this returns a 2d array, but we only need the first element) %>
	</div>
</div>
